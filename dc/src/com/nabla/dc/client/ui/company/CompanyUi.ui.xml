<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default"
	xmlns:wnd="urn:import:com.nabla.wapp.client.ui"
	>

	<wnd:Tab title="Company" icon="company_small.png" canClose="true">
		<ui:attribute name="title" description="Tab title"/>
		<wnd:VLayout>
			<wnd:Toolbar width="100%">
					<wnd:ToolbarButton tooltip="Add role"><ui:msg>Sales Ledger</ui:msg></wnd:ToolbarButton>
					<wnd:ToolbarButton tooltip="Remove role"><ui:msg>Purchase Ledger</ui:msg></wnd:ToolbarButton>
			</wnd:Toolbar>
			<wnd:TabDisplaySet ui:field="tabs" width="100%" height="100%">
			</wnd:TabDisplaySet>
		</wnd:VLayout>
	</wnd:Tab>
</ui:UiBinder> 