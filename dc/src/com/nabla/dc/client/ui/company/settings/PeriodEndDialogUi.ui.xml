<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default"
    xmlns:ctrl="urn:import:com.nabla.wapp.client.ui"
	xmlns:wnd="urn:import:com.nabla.wapp.client.ui"
	xmlns:form="urn:import:com.nabla.wapp.client.ui.form">
	
	<ui:with field="model" type="com.nabla.dc.client.model.company.settings.PeriodEndTreeModel"/>
	
	<wnd:ModalDialog caption="Period Ends" icon="period_end_small.png" autoClose="true">
		<ui:attribute name="caption" description="Edit company period ends dialog caption"/>
		<form:Form ui:field="form" autoFetchData="false" width="450" height="350">
			<form:ColumnTreeItem ui:field="tree" model="{model}" title="Period Ends" colSpan="2" width="*" height="200" loadDataOnDemand="true" firstColumnTitle="Financial Year">
				<ui:attribute name="title" description="Period Ends message"/>
			</form:ColumnTreeItem>
			<form:ButtonBar>
				<wnd:CancelButton><ui:msg description="Cancel button">*Close</ui:msg></wnd:CancelButton>
			</form:ButtonBar>
		</form:Form>	
	</wnd:ModalDialog>
</ui:UiBinder> 